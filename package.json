{
  "name": "devslopes-api-course",
  "version": "0.0.1",
  "description": "Devslopes RESTful API Course API Project",
  "main": "dist",
  "scripts": {
    "dev": "setx NODE_ENV=development & nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "setx NODE_ENV=production & pm2 start dist",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dalgurak/API.git"
  },
  "author": "Velimir Gerasimov <velimir.gerasimov@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^4.18.2",
    "mocha": "^7.1.2",
    "nodemon": "^1.11.0",
    "should": "^13.2.3",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "express-jwt": "^5.3.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^4.6.6",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "path": "^0.12.7"
  },
  "keywords": [
    "express",
    "es6",
    "rest",
    "api"
  ]
}
